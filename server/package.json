{
  "name": "api-async-server",
  "version": "1.0.0",
  "description": "api async server",
  "scripts": {
    "babel-node": "babel-node ./src/index.js",
    "build:server:dev": "NODE_ENV=development BABEL_ENV=development webpack --config webpack/webpack.server.babel.js",
    "dev": "npm run build:server:dev && npm run start",
    "dev:server": "nodemon src/index.js --exec babel-node",
    "start": "node ./dist/server/index.js",
    "start:dev": "NODE_ENV=development wait-on ./dist/server && nodemon --watch ./dist/server",
    "start:inspect": "npm run lint && npm run build && nodemon --inspect --exec npm run babel-node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --fix ./",
    "init": "source init.sh",
    "clean": "rm -rf node_modules && rm -rf dist",
    "clean:install": "npm run clean && npm install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "assets-webpack-plugin": "3.8.3",
    "babel-loader": "7.1.4",
    "clean-webpack-plugin": "0.1.19",
    "express": "4.16.3",
    "html-webpack-harddisk-plugin": "0.2.0",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-pug-plugin": "0.3.0",
    "webpack": "4.12.0",
    "webpack-disk-plugin": "0.0.2",
    "webpack-merge": "4.1.3",
    "webpack-node-externals": "1.7.2",
    "write-assets-webpack-plugin": "1.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-standard": "1.6.6",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.9.1",
    "eslint-plugin-standard": "3.1.0",
    "nodemon": "1.17.5",
    "style-loader": "0.21.0",
    "wait-on": "2.1.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  }
}
